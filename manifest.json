{
  "manifest_version": 3,
  "name": "My Private Extension",
  "version": "1.2",
  "description": "A private extension for my web app",
  "permissions": [
    "activeTab",
    "storage",
    "downloads",
    "notifications",
    "management"
  ],
  "background": {
    "service_worker": "dist/background.bundle.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "src/icons/icon16.png",
      "48": "src/icons/icon48.png",
      "128": "src/icons/icon128.png"
    }
  },
  "update_url": "https://lukaszsiepsiak.github.io/myExtension/update.xml",
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}
